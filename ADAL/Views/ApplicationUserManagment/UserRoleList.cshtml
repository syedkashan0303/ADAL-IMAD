@model List<ADAL.Models.DTO.ClientRoleMapping>

@{
    ViewData["Title"] = "Application Users";
}

<h1>Application Users</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Role Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@(item.Id ?? "N/A")</td>
                <td>@(item.Name ?? "N/A")</td>
                <td>@(item.RoleName ?? "N/A")</td>
            </tr>
        }
    </tbody>
</table>
